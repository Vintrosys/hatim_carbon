 "html": "<style>\ntable {\n    width: 100%;\n    border-collapse: collapse;\n    font-family: Arial;\n    font-size: 11px;\n}\ntd, th {\n    border: 1px solid #000;\n    padding: 6px;\n}\n.title {\n    font-weight: bold;\n    text-align: center;\n    font-size: 13px;\n}\n</style>\n\n{% for row in doc.inner_boxes %}\n<table>\n    <tr>\n        <th colspan=\"2\" class=\"title\">INNER BOX LABEL</th>\n    </tr>\n    <tr>\n        <td><b>Shipment No</b></td>\n        <td>{{ doc.name }}</td>\n    </tr>\n    <tr>\n        <td><b>Outer Box</b></td>\n        <td>{{ row.outer_box }}</td>\n    </tr>\n    <tr>\n        <td><b>Delivery Note</b></td>\n        <td>{{ row.delivery_note }}</td>\n    </tr>\n    <tr>\n        <td><b>Item Code</b></td>\n        <td>{{ row.item_code }}</td>\n    </tr>\n    <tr>\n        <td><b>Description</b></td>\n        <td>{{ row.item_description }}</td>\n    </tr>\n    <tr>\n        <td><b>Quantity</b></td>\n        <td>{{ row.qty }}</td>\n    </tr>\n    <tr>\n        <td><b>Net Weight</b></td>\n        <td>{{ row.net_weight }} kg</td>\n    </tr>\n    <tr>\n        <td><b>Gross Weight</b></td>\n        <td>{{ row.gross_weight }} kg</td>\n    </tr>\n    <tr>\n        <td><b>Customer</b></td>\n        <td>{{ doc.delivery_customer }}</td>\n    </tr>\n</table>\n\n<div style=\"page-break-after: always;\"></div>\n{% endfor %}\n",
